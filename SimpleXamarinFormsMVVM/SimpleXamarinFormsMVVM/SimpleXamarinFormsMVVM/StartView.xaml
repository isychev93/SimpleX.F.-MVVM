<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SimpleXamarinFormsMVVM.StartView">
  <StackLayout VerticalOptions="Center" HorizontalOptions="Center" Orientation="Vertical">
    <Button Text="Back"  Command="{Binding GoBackCommand}"/>
    <Button Text="Next"  Command="{Binding NextPageCommand}"/>
    <Button Text="Master"  Command="{Binding MasterPageCommand}"/>
  </StackLayout>
</ContentPage>